<div xmlns="http://www.w3.org/1999/xhtml"
      xmlns:meta="http://xml.zope.org/namespaces/meta"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      meta:interpolation="true"
      tal:omit-tag="">
  <ul id="tabs">
    <li tal:repeat="tab view/tabs" class="${tab/class}"><a href="${tab/url}">${tab/title}</a></li>
    <li id="home"><a href="${view/home_url}">Home</a></li>
    <li id="search">
      <form>
        <fieldset>
          <label for="searchField" class="superImpose">Search</label><input type="text" name="searchField" id="searchField" /><button type="submit">Submit</button>
        </fieldset>
      </form>
    </li>
  </ul>
</div>
