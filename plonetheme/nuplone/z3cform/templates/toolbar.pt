<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:tal="http://xml.zope.org/namespaces/tal">
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link rel="stylesheet" type="text/css" media="all" href="http://www.2style4you.com/static61/public/cms/presentation/drawer/style/base.css"/>
  </head>
  <body id="editBar">
    <div id="buttonBar">
      <div id="editorFunctions">
        <fieldset>
          <button type="button" disabled="disabled" class="inline strong">B</button>
          <button type="button" disabled="disabled" class="inline emphasised">I</button>
        </fieldset>

        <fieldset class="buttonGroup">
          <button type="button" disabled="disabled" class="icon unorderedList">Unordered List</button>
          <button type="button" disabled="disabled" class="icon orderedList">Ordered List</button>
        </fieldset>

        <fieldset class="buttonGroup">
          <button disabled="disabled" type="button" class="icon subscript">Subscript</button>
          <button disabled="disabled" type="button" class="icon superscript">Superscript</button>
        </fieldset>
      </div>
    </div>
    <script type="text/javascript" tal:attributes="src context/++resource++NuPlone.libraries/jquery-1.4.2.js"></script>
    <script type="text/javascript">
      function getTiny() {
          var topDoc = window.frameElement.ownerDocument,
              topWindow = topDoc.defaultView!==undefined ? topdoc.defaultView : topdoc.parentWindow;
          return topWindow.tinyMCE;
      }

      $("#editorFunctions .strong").live("click", function() {
          getTiny().execCommand("Bold"); return false; });
      $("#editorFunctions .emphasised").live("click", function() {
          getTiny().execCommand("italic"); return false; });

      $("#editorFunctions .unorderedList").live("click", function() {
          getTiny().execCommand("InsertUnorderedList"); return false; });
      $("#editorFunctions .orderedList").live("click", function() {
          getTiny().execCommand("InsertOrderedList"); return false; });

      $("#editorFunctions .subscript").live("click", function() {
          getTiny().execCommand("Subscript"); return false; });
      $("#editorFunctions .superscript").live("click", function() {
          getTiny().execCommand("Superscript"); return false; });
    </script>
  </body>
</html>

